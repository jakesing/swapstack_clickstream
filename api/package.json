{
  "name": "swapstack-s3",
  "version": "1.0.0",
  "description": "Swapstack S3",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "local:dev:db": "docker compose -f docker-compose.db.yml up -d",
    "local:dev:db:down": "docker compose -f docker-compose.db.yml down",
    "dev": "env TZ='UTC' npx serverless offline --stage dev --region us-east-1 --reloadHandler",
    "local:deploy": "npx serverless deploy --stage local --region us-east-1",
    "stage:deploy": "npx serverless deploy --stage stage --region us-east-1",
    "prettier:format": "prettier --write \"src/**/*.{ts,jsx,js,css,json,css,scss,md}\"",
    "prettier:check": "CI=true npx prettier \"src/**/*.{ts,jsx,js,css,json,css,scss,md}\" --check",
    "migrate:make": "knex migrate:make -x ts --knexfile src/config/knexfile.ts --env local",
    "migrate:latest": "npx knex migrate:latest --knexfile src/config/knexfile.ts --env local",
    "migrate:rollback": "npx knex migrate:rollback --knexfile src/config/knexfile.ts --env local",
    "seed:run": "env TZ='UTC' knex seed:run --knexfile src/config/knexfile.ts --env local",
    "migrate:make:stage": "knex migrate:make -x ts --knexfile src/config/knexfile.ts --env stage",
    "migrate:latest:stage": "npx knex migrate:latest --knexfile src/config/knexfile.ts --env stage",
    "seed:run:stage": "env TZ='UTC' knex seed:run --knexfile src/config/knexfile.ts --env stage",
    "migrate:make:live": "knex migrate:make -x ts --knexfile src/config/knexfile.ts --env live",
    "migrate:latest:live": "npx knex migrate:latest --knexfile src/config/knexfile.ts --env live",
    "migrate:rollback:live": "npx knex migrate:rollback --knexfile src/config/knexfile.ts --env live",
    "seed:run:live": "env TZ='UTC' knex seed:run --knexfile src/config/knexfile.ts --env live"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.329.0",
    "@aws-sdk/client-sqs": "^3.329.0",
    "@middy/core": "^4.4.0",
    "@middy/do-not-wait-for-empty-event-loop": "^4.4.0",
    "@middy/error-logger": "^4.4.0",
    "@middy/http-cors": "^4.4.0",
    "@middy/http-json-body-parser": "^4.4.0",
    "@middy/http-router": "^4.4.0",
    "@middy/http-security-headers": "^4.4.0",
    "@middy/sqs-partial-batch-failure": "^4.4.0",
    "date-fns": "^2.30.0",
    "http-status": "^1.6.2",
    "jsonrepair": "^3.1.0",
    "knex": "^2.4.2",
    "mysql2": "^3.3.0",
    "serverless-api-gateway-throttling": "^2.0.3",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.29.0",
    "@types/aws-lambda": "^8.10.115",
    "@types/node": "^20.1.0",
    "dotenv": "^16.0.3",
    "esbuild": "0.16.17",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "p-limit": "3.1.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "serverless": "^3.30.1",
    "serverless-add-api-key": "^4.2.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.44.0",
    "serverless-offline": "^12.0.4",
    "serverless-prune-plugin": "^2.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}